{
  "version": 2,
  "name": "ml-pipeline-api",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
