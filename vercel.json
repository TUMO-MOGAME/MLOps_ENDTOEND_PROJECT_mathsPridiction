{
  "version": 2,
  "name": "ml-pipeline-api",
  "builds": [
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/test.py"
    },
    {
      "src": "/",
      "dest": "/api/test.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
